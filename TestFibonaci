public class TestFibonacci {
 
	public static void main(String args[])
	{		
		int prev, next, sum, n;
		prev= next= 1;
		for(n=1;n<=10;n++) // n= 1 to 10
		{
			System.out.println(prev);
			sum=prev+next;
			prev=next;
			next=sum;
		}
	}
}

1 // F(1)=1
1 // F(2)=F(1)=1
2 // F(3)=F(2)+F(1)=1+1=2
3 // F(4)=F(3)+F(2)=2+1=3
5 // F(5)=F(4)+F(3)=3+2=5
8 // F(6)=F(5)+F(4)=5+3=8
13 // F(7)=F(6)+F(5)=8+5=13
21 // F(8)=F(7)+F(6)=13+8=21
34 // F(9)=F(8)+F(7)=21+13=34
55 // F(10)=F(9)+F(8)=34+21=55
